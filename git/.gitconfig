[color]
    ui = true
[push]
    default = current

[pull]
    default = current

[core]
    excludesfile = ~/.gitignore_global
    editor = vim +start +'normal $'

[flake8]
    complexity = -1
    strict = true
    ignore =
    lazy = false

[alias]
	up = !git fetch && git rebase --autostash FETCH_HEAD

[init]
	templatedir = ~/.git_templates

[color "diff"]
	meta = yellow black
